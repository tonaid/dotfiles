urxvt*transparent:true
urxvt*shading: 9
urxvt*scrollBar:false
urxvt*font:xft:Dejavu Sans Mono:size=11
urxvt*letterSpace: -1

Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

! special
*.foreground:   #c5c8c6
*.background:   #1d1f21
*.cursorColor:  #c5c8c6

! black
*.color0:       #393939
*.color8:       #373b41

! red
*.color1:       #f41e1e
*.color9:       #ff5656

! green
*.color2:       #553f30
*.color10:      #fc9c5a

! yellow
*.color3:       #73563d
*.color11:      #ff8012

! blue
*.color4:       #b8bcb4
*.color12:      #e4ffca

! magenta
*.color5:       #5d6661
*.color13:      #9ab0a4

! cyan
*.color6:       #5e8d87
*.color14:      #8abeb7

! white
*.color7:       #707880
*.color15:      #c5c8c6


