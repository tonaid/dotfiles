urxvt*transparent:true
urxvt*shading: 25
urxvt*scrollBar:false
urxvt*font:xft:Dejavu Sans Mono:size=11
urxvt*letterSpace: -1

Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

! special
*.foreground:   #c5c8c6
*.background:   #1d1f21
*.cursorColor:  #c5c8c6

! black
*.color0:       #282a2e
*.color8:       #373b41

! red
*.color1:       #a54242
*.color9:       #cc6666

! green
*.color2:       #8C9440
*.color10:      #B5BD68

! yellow
*.color3:       #de935f
*.color11:      #f0c674

! blue
*.color4:       #4771BA
*.color12:      #5CACEF

! magenta
*.color5:       #85678F
*.color13:      #AA73BB

! cyan
*.color6:       #5e8d87
*.color14:      #8abeb7

! white
*.color7:       #b3b7bc
*.color15:      #c5c8c6
